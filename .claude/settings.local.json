{
  "defaultMode": "bypassPermissions",
  "permissions": {
    "allow": [
      "Bash(xcodebuild:*)",
      "Bash(swift package:*)",
      "Bash(find:*)",
      "Bash(timeout 60 xcodebuild:*)",
      "Bash(rm:*)",
      "Bash(xcrun simctl list:*)",
      "Bash(xcrun simctl shutdown:*)",
      "Bash(xcrun simctl boot:*)",
      "Bash(xcrun simctl uninstall:*)",
      "Bash(true)"
    ],
    "deny": []
  }
}